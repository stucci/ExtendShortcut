{
    "name": "Extend Shortcut",
    "version": "0.0.0.2",
    "manifest_version": 2,
    "description": "Extend Shortcut key",
    "icons": {
      "128": "icon/keyboard.png"
    },
    "content_scripts": [
        {
        "matches": [ "https://www.google.co.jp/search?*",
                   "https://www.google.com/search?*",
                     "https://www.google.co.jp/webhp?sourceid=*" ],
        "js": [ "content_script.js" ],
        "run_at": "document_end"
        }
    ]
}
